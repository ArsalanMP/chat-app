.container
  position: absolute
  left 300px
  background-color: #e1e1e1
  box-sizing: border-box
  display: inline-block
  overflow-y: scroll
  background: url(https://il2.picdn.net/shutterstock/videos/1193104/thumb/1.jpg)
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
  bottom: 45px
  top 0
  right: 0
  padding: 30px

.myBtn:hover
  opacity: 0.9
  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19)

.myInput
  width: 100%
  height: 100%
  padding: 12px 20px
  border: 1px solid #ccc
  box-sizing: border-box

.column
  position: fixed
  display inline-block
  overflow-y: scroll
  width 300px
  height: 100%

.chatForm
  position: fixed
  height: 45px
  left 300px
  bottom: 0
  right: 0
  
.listDiv
  width: 100%